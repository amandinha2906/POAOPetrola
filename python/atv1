def ler_vetor():
    vetor = []
    for i in range(15):
        numero = int(input(f"Digite o {i+1}º número inteiro: "))
        vetor.append(numero)
    return vetor

def mostrar_vetorinverso(vetor):
    print("Vetor lido em ordem inversa:")
    for numero in reversed(vetor):
        print(numero, end=" ")

def main():
    vetor = ler_vetor()
    mostrar_vetorinverso(vetor)

if __name__ == "__main__":
    main()
